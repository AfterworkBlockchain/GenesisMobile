{
  "expo": {
    "name": "Genesis Mobile",
    "description": "Genesis Space Mobile Client",
    "slug": "genesisMobile",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "0.3.0",
    "orientation": "portrait",
    "icon": "./assets/appIcon/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.theGenesisSpace.GenesisMobile"
    },
    "android": {
      "package": "com.theGenesisSpace.GenesisMobile"
    }
  }
}
